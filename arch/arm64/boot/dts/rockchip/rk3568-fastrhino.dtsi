	status = "okay";
};

&pcie30phy {
	status = "okay";
};

&pcie3x1 {
	rockchip,bifurcation;
	reset-gpios = <&gpio0 RK_PC3 GPIO_ACTIVE_HIGH>;
	vpcie3v3-supply = <&vcc3v3_pcie>;
	status = "okay";

	pcie@0,0 {
		reg = <0x00100000 0 0 0 0>;
		#address-cells = <3>;
		#size-cells = <2>;

		rtl8125_1: pcie-eth@10,0 {
			compatible = "pci10ec,8125";
			reg = <0x000000 0 0 0 0>;

			/* G+ 3v3
			 * G- LED1: LINK on 100 + 1000 + 2500
			 * Y+ 3v3
			 * Y- LED0: ACT on all speed
			 */
			r8125,led0 = <0x0200>;
			r8125,led1 = <0x002a>;
		};
	};
};

&pcie3x2 {
	rockchip,bifurcation;
	reset-gpios = <&gpio0 RK_PC6 GPIO_ACTIVE_HIGH>;
	vpcie3v3-supply = <&vcc3v3_pcie>;
	status = "okay";

	pcie@0,0 {
		reg = <0x00200000 0 0 0 0>;
		#address-cells = <3>;
		#size-cells = <2>;

		rtl8125_2: pcie-eth@20,0 {
			compatible = "pci10ec,8125";
			reg = <0x000000 0 0 0 0>;

			/* G+ 3v3
			 * G- LED1: LINK on 100 + 1000 + 2500
			 * Y+ 3v3
			 * Y- LED0: ACT on all speed
			 */
			r8125,led0 = <0x0200>;
			r8125,led1 = <0x002a>;
		};
	};
};

